{
  "appInstanceName": "${appInstanceName}",
  "appTemplateId": "${appTemplateId}",
  "siteId": "${siteId}",
  "deploymentPlan": {
    "appServices": {
      "${svcName}": {
        "enabled": true,
        "space": "${space}",
        "artifactRegistryName": "${artifactRegistryName}",
        "imageVersion": "${svcVersion}"
      }
    },
    "dataServices": {
      "${dataServiceName}": {
        "dsbURN": "${dsbUrn}",
        "enabled": true,
        "dsbPlan": "${dsbPlan}",
        "dsbProtocol": "${dsbProtocol}"
      }
    }
  }
}
